<h2>商品一覧</h2>
<table>
 <thead>
  <th>商品ID</th>
  <th>商品名</th>
  <th>税抜価格</th>
  <th>ジャンル</th>
  <th>ステータス</th>
 </thead>
 <% @items.each do |item| %>
 <tbody>
  <td><%= item.id %></td>
  <td><%=link_to item.name, admin_item_path(item.id) %></td>
  <td><%= item.price %></td>
  <td><%= item.genre_id %></td>
  <td><%= item.is_active %></td>
 </tbody>
</table>
